{
    "version": 2,
    "builds": [
        {
        "src": "src/index.ts",
        "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/index.ts"
        }
    ],
    "headers": [
        {
        "key":"x-authorized",
        "value":"true",
        "Access-Control-Allow-Origin": "*"
        }
    ]
}